function GMap(){var i={lat:39.41308,lng:35.33585},t,n;this.initScript=function(){window.gMapScriptLoaded||(Common.loadScript("https://maps.googleapis.com/maps/api/js?key="+window.mapApiKey+"&libraries=places"),window.gMapScriptLoaded=!0)};this.setDefaultLocation=function(n){var t=$(n).val().replace(" ","").split(",");t.length>1&&(i={lat:parseFloat(t[0]),lng:parseFloat(t[1])})};this.gMapInitAutocomplete=function(r,u){var o,f,e;return window.gMap.isInitialized?!1:(window.gMap.isInitialized=!0,n=new google.maps.Map(document.getElementById("gmap"),{center:i,zoom:u,mapTypeId:google.maps.MapTypeId.ROADMAP}),o=document.getElementById("pac-input"),f=new google.maps.places.SearchBox(o),n.controls[google.maps.ControlPosition.TOP_LEFT].push(o),n.addListener("zoom_changed",function(){var t=$(r).val(),i=t.split(",")[0],u=t.split(",")[1];$(r).val(i+","+u+","+n.getZoom())}),n.addListener("bounds_changed",function(){f.setBounds(n.getBounds())}),t=new google.maps.Marker({animation:google.maps.Animation.BOUNCE,position:i,map:n,draggable:!0}),e=[t],f.addListener("places_changed",function(){var i=f.getPlaces(),u;i.length!=0&&(i=[i[0]],t.setMap(null),e.forEach(function(n){n.setMap(null)}),e=[],u=new google.maps.LatLngBounds,i.forEach(function(t){var i=new google.maps.Marker({map:n,title:t.name,draggable:!0,animation:google.maps.Animation.BOUNCE,position:t.geometry.location});e.push(i);t.geometry.viewport?u.union(t.geometry.viewport):u.extend(t.geometry.location);$(r).val(""+t.geometry.location.lat().toFixed(5)+","+t.geometry.location.lng().toFixed(5)+","+n.getZoom());google.maps.event.addListener(i,"dragend",function(t){$(r).val(""+t.latLng.lat().toFixed(5)+","+t.latLng.lng().toFixed(5)+","+n.getZoom())})}),n.fitBounds(u))}),google.maps.event.addListener(t,"dragend",function(t){$(r).val(""+t.latLng.lat().toFixed(5)+","+t.latLng.lng().toFixed(5)+","+n.getZoom())}),!1)};this.search=function(n,t,i,r,u){var f="",e;t=t==null?16:t;i!=null?f=i:r!=null&&u!=null&&r.length>0&&u.length>0?(f=u+", "